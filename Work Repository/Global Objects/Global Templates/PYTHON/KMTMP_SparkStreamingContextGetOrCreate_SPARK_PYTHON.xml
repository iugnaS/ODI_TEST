from pyspark.streaming import StreamingContext
ssc = None
{# IF ($[CHECKPOINTING] == 'true') #}
# Restart from Checkpoint
if config.get("spark.restartFromCheckpoint").lower() == "true" and config.get("spark.checkpointingBaseDir") != None and config.get("spark.checkpointingBaseDir") != "":
    print 'Restart from checkpointing dir:' + config.get("spark.checkpointingBaseDir") + "/" + "$[CURRENT_CHECKPOINT_ID]"
    try:
      ssc  = StreamingContext.getOrCreate(config.get("spark.checkpointingBaseDir") + "/" + "$[CURRENT_CHECKPOINT_ID]", None)
    except TypeError:
      print("Warning: Restart from checkpointing failed\n")
    if ssc != None:
      print "checkpointing identifier:" + ssc.sparkContext._conf.get("spark.checkpoint.id")
      if ssc.sparkContext._conf.get("spark.checkpoint.id") != "$[CURRENT_CHECKPOINT_ID]":
          print "Error: Restore from checkpoint failed: Checkpoint identifier does not match current mapping/physical design."
          exit(1)
      runApplication(ssc, config)
      exit(0)
{# ENDIF #}
if ssc == None:
    ssc = StreamingContext(sc, int(config.get("spark.batchDuration")))
